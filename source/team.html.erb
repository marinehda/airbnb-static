<div class= "container text-center padded">
  <h1 class="team-title">Our team</h1>
  <ul class="list-inline text-center">
    <% ["fabien", "marine", "leo", "julius"].each do |member| %>
      <li>
        <%= image_tag "#{member}.png", class: "avatar-mega-large team-list" %>
